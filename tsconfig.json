{
	"compilerOptions": {
		"moduleResolution": "node",
		"module": "commonjs",
		"target": "es2020",
		"jsx": "preserve",
		"strictFunctionTypes": true,
		"sourceMap": true,
		"plugins": [
			{
				"transform": "./exp3/transformer.ts"
			}
		]
	},
	"exclude": [
		"node_modules",
		"**/node_modules/*"
	]
}